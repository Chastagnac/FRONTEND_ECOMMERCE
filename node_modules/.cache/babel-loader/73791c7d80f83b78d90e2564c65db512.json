rmSearch()\r\n        }\r\n    },\r\n    methods : {\r\n        async performSearch() {\r\n            this.$store.commit('setIsLoading', true)\r\n            \r\n            await axios\r\n                .post('/api/v1/products/search/', {'query': this.query})\r\n                .then(response => {\r\n                    this.products = response.data\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                })\r\n\r\n            this.$store.commit('setIsLoading', false)\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}