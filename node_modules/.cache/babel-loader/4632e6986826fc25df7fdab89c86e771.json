AAA,EAiBE,CAhBE,YAAA,CAAyE,sBAAzE,EAAyE;AAA5D,IAAA,EAAE,EAAC,cAAyD;AAA1C,IAAA,KAAK,EAAC;AAAoC,GAAzE,E;sBAAmD;AAAA,aAAQ,C,UAAA,CAAR;AAAA,K;;;;GAAnD,CAgBF,EAfF,mBAAA,CAcM,KAdN,EAAA,WAAA,EAcM,CAbF,mBAAA,CAYM,KAZN,EAAA,WAAA,EAYM,CAXS,IAAA,CAAA,MAAA,CAAO,KAAP,CAAa,e,kBAAxB,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADF,YAAA,CAA6E,sBAA7E,EAA6E;AAAhE,IAAA,EAAE,EAAC,YAA6D;AAAhD,IAAA,KAAK,EAAC;AAA0C,GAA7E,E;sBAAqD;AAAA,aAAU,C,WAAA,CAAV;AAAA,K;;;;GAArD,CACE,CAFN,C,mBAGA,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADF,YAAA,CAA4E,sBAA5E,EAA4E;AAA/D,IAAA,EAAE,EAAC,SAA4D;AAAlD,IAAA,KAAK,EAAC;AAA4C,GAA5E,E;sBAAkD;AAAA,aAAY,C,WAAA,CAAZ;AAAA,K;;;;GAAlD,CACE,CAFN,C,CAQE,EAJF,YAAA,CAGc,sBAHd,EAGc;AAHD,IAAA,EAAE,EAAC,OAGF;AAHU,IAAA,KAAK,EAAC;AAGhB,GAHd,E;sBACI;AAAA,aAA8D,CAA9D,WAA8D,EAC9D,mBAAA,CAAyC,MAAzC,EAAyC,IAAzC,EAAM,WAAM,gBAAA,CAAG,QAAA,CAAA,eAAH,CAAN,GAA2B,GAAjC,EAAkC;AAAA;AAAlC,OAD8D,CAA9D;AAAA,K;;;;GADJ,CAIE,CAZN,CAaE,CAdN,CAeE,CAjBF,CAkBF,CAnCF,CAoCE,CAhDN,CA0DF,EATF,mBAAA,CAEM,KAFN,EAEM;AAFD,IAAA,KAAK,EAAA,eAAA,CAAA,CAAC,kCAAD,EAAmC;AAAA,oBAA8B,IAAA,CAAA,MAAA,CAAO,KAAP,CAAa;AAA3C,KAAnC,CAAA;AAEJ,GAFN,E,WAAA,E;;AAAA,GASE,EALF,mBAAA,CAEU,SAFV,EAAA,WAAA,EAEU,CADN,YAAA,CAAe,sBAAf,CACM,CAFV,CAKE,CA3DF,C","sourcesContent":["<template>\r\n    <div id=\"wrapper\">\r\n        <nav class=\"navbar is-dark\">\r\n            <div class=\"navbar-brand\">\r\n                <router-link to=\"/\" class=\"navbar-item\">\r\n                    <strong>Se digitaliser</strong>\r\n                </router-link>\r\n                <a class=\"navbar-burger\" aria-label=\"menu\" aria-expanded=\"false\" data-target=\"navbar-menu\">\r\n                    <span aria-hidden=\"true\"></span>\r\n                    <span aria-hidden=\"true\"></span>\r\n                    <span aria-hidden=\"true\"></span>\r\n                </a>\r\n            </div>\r\n\r\n            <div class=\"navbar-menu\" id=\"navbar-menu\">\r\n                <div class=\"navbar-start\">\r\n                    <div class=\"navbar-item\">\r\n                        <form method=\"get\" action=\"/search\">\r\n                            <div class=\"field has-addons\">\r\n                                <input type=\"text\" class=\"input\" placeholder=\"Que recherchez vous ?\" name=\"query\">\r\n                                <div class=\"control\">\r\n                                    <button class=\"button is-success\">\r\n                                        <span class=\"icon\">\r\n                                            <i class=\"fas fa-search\"></i>\r\n                                        </span>\r\n                                    </button>\r\n                                </div>\r\n                            </div>\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n                <div class=\"navbar-end\">\r\n                    <router-link to=\"/box-country\" class=\"navbar-item\">Box Pays</router-link>\r\n                <div class=\"navbar-item\">\r\n                    <div class=\"buttons\">\r\n                        <div v-if=\"$store.state.isAuthenticated\" class=\"mr-1\">\r\n                            <router-link to=\"/MyAccount\" class=\"button is-light\">Mon Compte</router-link>\r\n                        </div>\r\n                        <div v-else class=\"mr-1\">\r\n                            <router-link to=\"/log-in\" class=\"button is-light\">Se connecter</router-link>\r\n                        </div>\r\n\r\n                        <router-link to=\"/cart\" class=\"button is-success\">\r\n                            <span class=\"icon\"><i class=\"fas fa-shopping-cart\"></i></span>\r\n                            <span>Cart ({{ cartTotalLenght }})</span>\r\n                        </router-link>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        </nav>\r\n    <div class=\"is-loading-bar has-text-centered\" v-bind:class=\"{'is-loading': $store.state.isLoading }\">\r\n        <div class=\"lds-dual-ring\"></div>\r\n    </div>\r\n\r\n    <section class=\"section\">\r\n        <router-view />\r\n    </section>\r\n\r\n\r\n</div>\r\n</template>\r\n\r\n<script> \r\nimport axios from 'axios'\r\n\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            showMobileMenu : false,\r\n            cart: {\r\n                items : []\r\n            }\r\n        }\r\n    },\r\n    // On initialize le localstorage avant les injections \r\n    beforeCreate() {\r\n        this.$store.commit('initializeStore')\r\n\r\n        const token = this.$store.token\r\n\r\n        if(token) {\r\n            axios.defaults.headers.common['Authorization'] = \"Token \" + token\r\n        } else {\r\n            axios.defaults.headers.common['Authorization'] = \"\"\r\n        }\r\n    },\r\n    // Quand l'app est \"montée\" on recupère $cart du store dans this.cart\r\n    mounted() {\r\n        this.cart = this.$store.state.cart\r\n    },\r\n    // Lorsque l'app tourne, si un produit viens s'add au panier,\r\n    //  il incrémente et retoure le total\r\n    computed: {\r\n        cartTotalLenght() {\r\n            let totalLenght = 0\r\n\r\n            for(let i = 0; i < this.cart.items.length; i++) {\r\n                totalLenght += this.cart.items[i].quantity\r\n            }\r\n            return totalLenght\r\n        }   \r\n    }\r\n}\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"scss\">\r\n@import \"../node_modules/bulma\";\r\n\r\n.lds-dual-ring {\r\n    display: inline-block;\r\n    width: 80px;\r\n    height: 80px;\r\n}\r\n.lds-dual-ring:after {\r\n    content: \" \";\r\n    display: block;\r\n    width: 64px;\r\n    height: 64px;\r\n    margin: 8px;\r\n    border-radius: 50%;\r\n    border: 6px solid #ccc;\r\n    border-color: #ccc transparent #ccc transparent;\r\n    animation: lds-dual-ring 1.2s linear infinite;\r\n}\r\n@keyframes lds-dual-ring {\r\n    0% {\r\n        transform: rotate(0deg);\r\n    }\r\n    100% {\r\n        transform: rotate(360deg);\r\n    }\r\n}\r\n\r\n.is-loading-bar {\r\n    height : 0;\r\n    overflow: hidden;\r\n\r\n    -webkit-transition: all 0.3s;\r\n    transition: all 0.3s;\r\n\r\n    &.is-loading {\r\n        height: 80px;\r\n    }\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""}]}